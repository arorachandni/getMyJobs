<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.escapia.com/EVRN/2007/02" targetNamespace="http://www.escapia.com/EVRN/2007/02" elementFormDefault="qualified">
	<xs:element name="EVRN_UnitDescriptiveInfoRQ">
		<xs:complexType mixed="false">
			<xs:complexContent mixed="false">
				<xs:extension base="tns:EVRN_BaseRQType">
					<xs:sequence>
						<xs:element name="UnitDescriptiveInfos" type="tns:ArrayOfUnitDescriptiveInfoRequestType" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="SummaryOnly" type="xs:boolean" default="false"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="EVRN_BaseRQType">
		<xs:sequence>
			<xs:element name="POS" type="tns:ArrayOfSourceType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="EchoToken" type="xs:string"/>
		<xs:attribute name="TimeStamp" type="xs:dateTime"/>
		<xs:attribute name="Target" default="Production">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Test"/>
					<xs:enumeration value="Production"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Version" type="xs:decimal" use="required"/>
		<xs:attribute name="TransactionIdentifier" type="xs:string"/>
		<xs:attribute name="SequenceNmbr" type="xs:nonNegativeInteger"/>
		<xs:attribute name="TransactionStatusCode">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Start"/>
					<xs:enumeration value="End"/>
					<xs:enumeration value="Rollback"/>
					<xs:enumeration value="InSeries"/>
					<xs:enumeration value="Continuation"/>
					<xs:enumeration value="Subsequent"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="RetransmissionIndicator" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="ArrayOfSourceType">
		<xs:sequence>
			<xs:element name="Source" type="tns:SourceType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SourceType">
		<xs:sequence>
			<xs:element name="RequestorID" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="ID" type="xs:string"/>
					<xs:attribute name="MessagePassword" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="BookingChannel" type="tns:BookingChannelType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BookingChannelType">
		<xs:sequence>
			<xs:element name="CompanyName" type="tns:CompanyNameType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string"/>
		<xs:attribute name="Primary" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="CompanyNameType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="CompanyShortName" type="xs:string"/>
				<xs:attribute name="TravelSector" type="xs:string"/>
				<xs:attribute name="Code" type="xs:string"/>
				<xs:attribute name="CodeContext" type="xs:string"/>
				<xs:attribute name="Division" type="xs:string"/>
				<xs:attribute name="Department" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="TravelArrangerType">
		<xs:simpleContent>
			<xs:extension base="tns:CompanyNameType">
				<xs:attribute name="DefaultInd" type="xs:boolean" default="false"/>
				<xs:attribute name="ShareSynchInd">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Yes"/>
							<xs:enumeration value="No"/>
							<xs:enumeration value="Inherit"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="ShareMarketInd">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Yes"/>
							<xs:enumeration value="No"/>
							<xs:enumeration value="Inherit"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="TravelArrangerType" type="xs:string"/>
				<xs:attribute name="RPH" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="CompanyNamePrefType">
		<xs:simpleContent>
			<xs:extension base="tns:CompanyNameType">
				<xs:attribute name="PreferLevel" type="tns:PreferLevelType" default="Preferred"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="PreferLevelType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Only"/>
			<xs:enumeration value="Unacceptable"/>
			<xs:enumeration value="Preferred"/>
			<xs:enumeration value="Required"/>
			<xs:enumeration value="NoPreference"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="UnitResRequestType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:EVRN_BaseRQType">
				<xs:sequence>
					<xs:element name="UniqueID" type="tns:UniqueID_Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="UnitReservations" type="tns:UnitReservationsType" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="ResStatus" type="tns:TransactionActionType"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="UniqueID_Type">
		<xs:sequence>
			<xs:element name="CompanyName" type="tns:CompanyNameType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="URL" type="xs:anyURI"/>
		<xs:attribute name="Type" type="xs:string"/>
		<xs:attribute name="Instance" type="xs:string"/>
		<xs:attribute name="ID" type="xs:string"/>
		<xs:attribute name="ID_Context" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="UnitReservationsType">
		<xs:sequence>
			<xs:element name="UnitReservation" type="tns:UnitReservationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RoutingHops" type="tns:ArrayOfRoutingHopTypeRoutingHop" minOccurs="0"/>
			<xs:element name="WrittenConfInst" type="tns:WrittenConfInstType" minOccurs="0"/>
			<xs:element name="TPA_Extensions" type="tns:TPA_Extensions_Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UnitReservationType">
		<xs:sequence>
			<xs:element name="UniqueID" type="tns:UniqueID_Type" minOccurs="0"/>
			<xs:element name="UnitStays" type="tns:ArrayOfUnitStaysTypeUnitStay" minOccurs="0"/>
			<xs:element name="Services" type="tns:ArrayOfServicesTypeService" minOccurs="0"/>
			<xs:element name="ResGuests" type="tns:ArrayOfResGuestsTypeResGuest" minOccurs="0"/>
			<xs:element name="ResGlobalInfo" type="tns:EVRN_ResGlobalInfoType" minOccurs="0"/>
			<xs:element name="WrittenConfInst" type="tns:WrittenConfInstType" minOccurs="0"/>
			<xs:element name="TPA_Extensions" type="tns:TPA_Extensions_Type" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="RoomStayReservation" type="xs:boolean"/>
		<xs:attribute name="CreateDateTime" type="xs:dateTime"/>
		<xs:attribute name="CreatorID" type="xs:string"/>
		<xs:attribute name="LastModifyDateTime" type="xs:dateTime"/>
		<xs:attribute name="LastModifierID" type="xs:string"/>
		<xs:attribute name="ResStatus" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ArrayOfUnitStaysTypeUnitStay">
		<xs:sequence>
			<xs:element name="UnitStay" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType mixed="false">
					<xs:complexContent mixed="false">
						<xs:extension base="tns:UnitStayType">
							<xs:sequence>
								<xs:element name="ResGuestRPHs" type="tns:ArrayOfResGuestRPHsTypeResGuestRPH" minOccurs="0"/>
								<xs:element name="Comments" type="tns:ArrayOfCommentTypeComment" minOccurs="0"/>
								<xs:element name="SpecialRequests" type="tns:ArrayOfSpecialRequestTypeSpecialRequest" minOccurs="0"/>
								<xs:element name="ServiceRPHs" type="tns:ArrayOfServiceRPHsTypeServiceRPH" minOccurs="0"/>
								<xs:element name="Reference" minOccurs="0">
									<xs:complexType mixed="false">
										<xs:complexContent mixed="false">
											<xs:extension base="tns:UniqueID_Type">
												<xs:attribute name="DateTime" type="xs:dateTime"/>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="IndexNumber" type="xs:integer"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UnitStayType">
		<xs:sequence>
			<xs:element name="UnitRates" type="tns:ArrayOfUnitStayTypeUnitRate" minOccurs="0"/>
			<xs:element name="GuestCounts" type="tns:GuestCountType" minOccurs="0"/>
			<xs:element name="TimeSpan" type="tns:DateTimeSpanType" minOccurs="0"/>
			<xs:element name="DepositPayments" type="tns:ArrayOfRequiredPaymentsTypeGuaranteePayment" minOccurs="0"/>
			<xs:element name="CancelPenalties" type="tns:CancelPenaltiesType" minOccurs="0"/>
			<xs:element name="Total" type="tns:TotalType" minOccurs="0"/>
			<xs:element name="BasicUnitInfo" type="tns:BasicUnitInfoType" minOccurs="0"/>
			<xs:element name="RentalAgreement" type="tns:ArrayOfParagraphType" minOccurs="0"/>
			<xs:element name="Pets" type="tns:PetsPoliciesType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ArrayOfUnitStayTypeUnitRate">
		<xs:sequence>
			<xs:element name="UnitRate" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Rates" type="tns:ArrayOfEVRN_AmountType" minOccurs="0"/>
						<xs:element name="UnitRateDescription" type="tns:ParagraphType" minOccurs="0"/>
						<xs:element name="Total" type="tns:TotalType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ArrayOfEVRN_AmountType">
		<xs:sequence>
			<xs:element name="Rate" type="tns:EVRN_AmountType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EVRN_AmountType">
		<xs:sequence>
			<xs:element name="Base" type="tns:TotalType" minOccurs="0"/>
			<xs:element name="AdditionalGuestAmounts" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AdditionalGuestAmount" type="tns:AdditionalGuestAmountType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="AmountBeforeTax" type="xs:decimal"/>
					<xs:attribute name="AmountAfterTax" type="xs:decimal"/>
					<xs:attribute name="CurrencyCode" type="xs:string"/>
					<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Fees" type="tns:ArrayOfFeeType" minOccurs="0"/>
			<xs:element name="CancelPolicies" type="tns:CancelPenaltiesType" minOccurs="0"/>
			<xs:element name="PaymentPolicies" type="tns:ArrayOfRequiredPaymentsTypeGuaranteePayment" minOccurs="0"/>
			<xs:element name="Discount" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType mixed="false">
					<xs:complexContent mixed="false">
						<xs:extension base="tns:DiscountType">
							<xs:attribute name="AppliesTo">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Base"/>
										<xs:enumeration value="AdditionalGuestAmount"/>
										<xs:enumeration value="Fee"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="ItemRPH" type="xs:string"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Total" type="tns:TotalType" minOccurs="0"/>
			<xs:element name="RateDescription" type="tns:ParagraphType" minOccurs="0"/>
			<xs:element name="AdditionalCharges" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AdditionalCharge" type="tns:AdditionalChargeType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="AmountBeforeTax" type="xs:decimal"/>
					<xs:attribute name="AmountAfterTax" type="xs:decimal"/>
					<xs:attribute name="CurrencyCode" type="xs:string"/>
					<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="AdvanceBookingRestriction" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="Start" type="xs:string"/>
					<xs:attribute name="Duration" type="xs:string"/>
					<xs:attribute name="End" type="xs:string"/>
					<xs:attribute name="MinAdvanceBookingOffset" type="xs:duration"/>
					<xs:attribute name="MaxAdvanceBookingOffset" type="xs:duration"/>
					<xs:attribute name="Mon" type="xs:boolean"/>
					<xs:attribute name="Tue" type="xs:boolean"/>
					<xs:attribute name="Weds" type="xs:boolean"/>
					<xs:attribute name="Thur" type="xs:boolean"/>
					<xs:attribute name="Fri" type="xs:boolean"/>
					<xs:attribute name="Sat" type="xs:boolean"/>
					<xs:attribute name="Sun" type="xs:boolean"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="EffectiveDate" type="xs:date"/>
		<xs:attribute name="ExpireDate" type="xs:date"/>
		<xs:attribute name="AgeQualifyingCode" type="xs:string"/>
		<xs:attribute name="MinAge" type="xs:integer"/>
		<xs:attribute name="MaxAge" type="xs:integer"/>
		<xs:attribute name="AgeTimeUnit" type="tns:TimeUnitType"/>
		<xs:attribute name="GuaranteedInd" type="xs:boolean"/>
		<xs:attribute name="NumberOfUnits" type="xs:integer"/>
		<xs:attribute name="RateTimeUnit" type="tns:TimeUnitType"/>
		<xs:attribute name="UnitMultiplier" type="xs:positiveInteger"/>
		<xs:attribute name="MinGuestApplicable" type="xs:positiveInteger"/>
		<xs:attribute name="MaxGuestApplicable" type="xs:positiveInteger"/>
		<xs:attribute name="MinLOS" type="xs:nonNegativeInteger"/>
		<xs:attribute name="MaxLOS" type="xs:nonNegativeInteger"/>
		<xs:attribute name="StayOverDate" type="tns:DayOfWeekType"/>
		<xs:attribute name="AlternateCurrencyInd" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="TotalType">
		<xs:sequence>
			<xs:element name="Taxes" type="tns:TaxesType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="AmountBeforeTax" type="xs:decimal"/>
		<xs:attribute name="AmountAfterTax" type="xs:decimal"/>
		<xs:attribute name="CurrencyCode" type="xs:string"/>
		<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger"/>
		<xs:attribute name="AdditionalFeesExcludedIndicator" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="TaxesType">
		<xs:sequence>
			<xs:element name="Tax" type="tns:TaxType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Amount" type="xs:decimal"/>
	</xs:complexType>
	<xs:complexType name="TaxType">
		<xs:sequence>
			<xs:element name="TaxDescription" type="tns:ParagraphType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Type" type="tns:AmountDeterminationType"/>
		<xs:attribute name="Code" type="xs:string"/>
		<xs:attribute name="Percent" type="xs:decimal"/>
		<xs:attribute name="Amount" type="xs:decimal"/>
		<xs:attribute name="CurrencyCode" type="xs:string"/>
		<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger"/>
		<xs:attribute name="EffectiveDate" type="xs:date"/>
		<xs:attribute name="ExpireDate" type="xs:date"/>
		<xs:attribute name="ChargeUnit" type="xs:string"/>
		<xs:attribute name="ChargeFrequency" type="xs:string"/>
		<xs:attribute name="ChargeUnitExempt" type="xs:positiveInteger"/>
		<xs:attribute name="ChargeFrequencyExempt" type="xs:positiveInteger"/>
		<xs:attribute name="MaxChargeUnitApplies" type="xs:positiveInteger"/>
		<xs:attribute name="MaxChargeFrequencyApplies" type="xs:positiveInteger"/>
	</xs:complexType>
	<xs:complexType name="ParagraphType">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Image" type="xs:string" minOccurs="0"/>
				<xs:element name="URL" type="xs:anyURI" minOccurs="0"/>
				<xs:element name="ListItem" minOccurs="0">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="tns:FormattedTextTextType">
								<xs:attribute name="ListItem" type="xs:integer"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Text" type="tns:FormattedTextTextType" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Name" type="xs:string"/>
		<xs:attribute name="ParagraphNumber" type="xs:nonNegativeInteger"/>
		<xs:attribute name="CreateDateTime" type="xs:dateTime"/>
		<xs:attribute name="CreatorID" type="xs:string"/>
		<xs:attribute name="LastModifyDateTime" type="xs:dateTime"/>
		<xs:attribute name="LastModifierID" type="xs:string"/>
		<xs:attribute name="Language" type="xs:language"/>
	</xs:complexType>
	<xs:complexType name="FormattedTextTextType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Formatted" type="xs:boolean"/>
				<xs:attribute name="Language" type="xs:language"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="AmountDeterminationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Inclusive"/>
			<xs:enumeration value="Exclusive"/>
			<xs:enumeration value="Cumulative"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DiscountType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:TotalType">
				<xs:sequence>
					<xs:element name="DiscountReason" type="tns:ParagraphType" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="TaxInclusive" type="xs:boolean"/>
				<xs:attribute name="Percent" type="xs:decimal"/>
				<xs:attribute name="DiscountCode" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RateDiscountType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:DiscountType">
				<xs:attribute name="MinAdvanceBookingOffset" type="xs:duration"/>
				<xs:attribute name="MaxAdvanceBookingOffset" type="xs:duration"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AdditionalGuestAmountType">
		<xs:sequence>
			<xs:element name="Amount" type="tns:TotalType" minOccurs="0"/>
			<xs:element name="AddlGuestAmtDescription" type="tns:ParagraphType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="MaxAdditionalGuests" type="xs:integer"/>
		<xs:attribute name="AgeQualifyingCode" type="xs:string"/>
		<xs:attribute name="MinAge" type="xs:integer"/>
		<xs:attribute name="MaxAge" type="xs:integer"/>
		<xs:attribute name="AgeTimeUnit" type="tns:TimeUnitType"/>
		<xs:attribute name="Type" type="xs:string"/>
		<xs:attribute name="Percent" type="xs:decimal"/>
		<xs:attribute name="RPH" type="xs:string"/>
	</xs:complexType>
	<xs:simpleType name="TimeUnitType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Year"/>
			<xs:enumeration value="Month"/>
			<xs:enumeration value="Week"/>
			<xs:enumeration value="Day"/>
			<xs:enumeration value="Hour"/>
			<xs:enumeration value="Second"/>
			<xs:enumeration value="FullDuration"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ArrayOfFeeType">
		<xs:sequence>
			<xs:element name="Fee" type="tns:FeeType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FeeType">
		<xs:sequence>
			<xs:element name="Taxes" type="tns:TaxesType" minOccurs="0"/>
			<xs:element name="Description" type="tns:ParagraphType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="TaxInclusive" type="xs:boolean"/>
		<xs:attribute name="Type" type="tns:AmountDeterminationType"/>
		<xs:attribute name="Code" type="xs:string"/>
		<xs:attribute name="Percent" type="xs:decimal"/>
		<xs:attribute name="Amount" type="xs:decimal"/>
		<xs:attribute name="CurrencyCode" type="xs:string"/>
		<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger"/>
		<xs:attribute name="EffectiveDate" type="xs:date"/>
		<xs:attribute name="ExpireDate" type="xs:date"/>
		<xs:attribute name="MandatoryIndicator" type="xs:boolean"/>
		<xs:attribute name="RPH" type="xs:string"/>
		<xs:attribute name="ChargeUnit" type="xs:string"/>
		<xs:attribute name="ChargeFrequency" type="xs:string"/>
		<xs:attribute name="ChargeUnitExempt" type="xs:positiveInteger"/>
		<xs:attribute name="ChargeFrequencyExempt" type="xs:positiveInteger"/>
		<xs:attribute name="MaxChargeUnitApplies" type="xs:positiveInteger"/>
		<xs:attribute name="MaxChargeFrequencyApplies" type="xs:positiveInteger"/>
		<xs:attribute name="TaxableIndicator" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="CommissionPolicyType" mixed="false">
		<xs:complexContent mixed="false">
			<xs:extension base="tns:FeeType">
				<xs:attribute name="MinAdvanceBookingOffset" type="xs:duration"/>
				<xs:attribute name="MaxAdvanceBookingOffset" type="xs:duration"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CancelPenaltiesType">
		<xs:sequence>
			<xs:element name="CancelPenalty" type="tns:CancelPenaltyType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="CancelPolicyIndicator" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="CancelPenaltyType">
		<xs:sequence>
			<xs:element name="Deadline" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="AbsoluteDeadline" type="xs:string"/>
					<xs:attribute name="OffsetTimeUnit" type="tns:TimeUnitType"/>
					<xs:attribute name="OffsetUnitMultiplier" type="xs:integer"/>
					<xs:attribute name="OffsetDropTime">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="BeforeArrival"/>
								<xs:enumeration value="AfterBooking"/>
